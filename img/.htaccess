Options +FollowSymlinks
RewriteEngine on

# If requested resource exists as a file or directory, skip next 1 rule
RewriteCond %{DOCUMENT_ROOT}/$1 -f [OR]
RewriteCond %{DOCUMENT_ROOT}/$1 -d
RewriteRule (.*) - [S=2]
#
RewriteRule ^([a-zA-Z0-9_-]+)/([0-9]+)\.jpg$ timthumb.php?src=upload/$1.jpg&w=$2px [QSA]
RewriteRule ^([a-zA-Z0-9_-]+)/([0-9]+)/([0-9]+)\.jpg$ timthumb.php?src=upload/$1.jpg&w=$2px&h=$3px [QSA]

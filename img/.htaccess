Options +FollowSymlinks
RewriteEngine on

# If requested resource exists as a file or directory, skip next 1 rule
RewriteCond %{DOCUMENT_ROOT}/$1 -f [OR]
RewriteCond %{DOCUMENT_ROOT}/$1 -d
RewriteRule (.*) - [S=2]
#
RewriteRule ^([0-9]+)/([a-zA-Z0-9_-]+)\.jpg$ timthumb.php?src=upload/$2.jpg&w=$1px [QSA]
RewriteRule ^([0-9]+)/([0-9]+)/([a-zA-Z0-9_-]+)\.jpg$ timthumb.php?src=upload/$3.jpg&w=$1px&h=$2px [QSA]
